{
  "manifest_version": 3,
  "name": "Filter XOXO",
  "description": "A Chrome extension that filters xoxo posts.",
  "version": "1.0",
  "host_permissions": [
    "https://www.xoxohth.com/*",
    "https://xoxohth.com/*",
    "http://xoxohth.com/*",
    "http://www.xoxohth.com/*",
    "https://*.xoxohth.com/*",
    "http://*.xoxohth.com/*",
    "https://www.autoadmit.com/*",
    "https://autoadmit.com/*",
    "http://autoadmit.com/*",
    "http://www.autoadmit.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.xoxohth.com/*",
        "https://xoxohth.com/*",
        "http://xoxohth.com/*",
        "http://www.xoxohth.com/*",
        "http://*.xoxohth.com/*",
        "https://*.xoxohth.com/*",
        "https://www.autoadmit.com/*",
        "https://autoadmit.com/*",
        "http://autoadmit.com/*",
        "http://www.autoadmit.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
